  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.7.1",
    "editorconfig": "^0.15.3",
    "editorconfig-checker": "^3.3.0",
    "tsc-watch": "^4.2.9",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3"
  }
