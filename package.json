{
  "name": "new-engine",
  "version": "3.0.5",
  "description": "new-engine is a HTML5 graphics engine",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "/build"
  ],
  "keywords": [
    "html5",
    "canvas",
    "html5 engine",
    "video gaming engine"
  ],
  "repository": {
    "type": "git",
